    "minimum-stability": "stable",
    "require": {
        "slim/slim": "^4.12",
        "slim/psr7": "^1.6",
        "psr/http-server-handler": "^1.0",
        "psr/http-message": "^2.0",
        "firebase/php-jwt": "^7.0",
        "vlucas/phpdotenv": "^5.6",
        "guzzlehttp/guzzle": "^7.10",
        "openai-php/client": "^0.19.0"
    },
    "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
